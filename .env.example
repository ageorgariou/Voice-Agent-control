# Airtable Configuration
VITE_AIRTABLE_API_KEY=your-airtable-api-key
VITE_AIRTABLE_BASE_ID=your-airtable-base-id

# Server Configuration
PORT=3001

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-make-it-at-least-32-characters-long
JWT_EXPIRES_IN=24h
REFRESH_TOKEN_EXPIRES_IN=7d

# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&appName=YourAppName
MONGODB_DB_NAME=voice_agent_db

# Node Environment
NODE_ENV=development
